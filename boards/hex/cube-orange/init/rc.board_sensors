#!/bin/sh
#
# Holybro Durandal V1 specific board sensors init
#------------------------------------------------------------------------------
adc start

# Internal SPI bus ICM-20948
# TODO: review driver
# icm20948 -s -R 6 start

# Baro on internal SPI1
ms5611 -s start

# External Vibration isolated sensors on SPI4
icm20649 -S -b 4 -c 1 -R 12 start
icm20602 -S -b 4 -c 2 -R 14 start

# Baro on external SPI4
ms5611 -S -b 4 -c 3 start

