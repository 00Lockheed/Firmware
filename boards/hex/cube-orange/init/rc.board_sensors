#!/bin/sh
#
# Cube Orange specific board sensors init
#------------------------------------------------------------------------------
adc start

icm20649 -s -R 12 start # Vibration isolated, SPI1

ms5611 -s start # SPI1

icm20602 -s -R 8 start # Vibration isolation, SPI4

# TODO: review driver
# TODO: Check/Fix Rotation
icm20948 -s -M -R 6 start 	# Not isolated, but also SPI4

ms5611 -S -b 4 -c 3 start # SPI4

